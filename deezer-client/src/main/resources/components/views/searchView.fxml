<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TabPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<fx:root xmlns:fx="http://javafx.com/fxml" type="javafx.scene.layout.VBox" fx:controller="components.views.SearchView">
    <TabPane fx:id="searchTabPane" tabClosingPolicy="UNAVAILABLE">
        <padding>
            <Insets top="25.0" />
        </padding>
        <Tab fx:id="allResultsTab" text="%all">
            <ScrollPane fitToWidth="true">
                <padding>
                    <Insets left="80.0" right="80.0" top="50.0" />
                </padding>
                <VBox fx:id="fullFoundBox">
                    <Button fx:id="tracksResultBtn" mnemonicParsing="false" onAction="#tracksResultBtn_OnAction" text="%tracks">
                        <styleClass>
                            <String fx:value="deezer-button" />
                            <String fx:value="deezer-subtitle" />
                        </styleClass>
                    </Button>
                    <TableView fx:id="tracksResultTV" VBox.vgrow="NEVER">
                        <columns>
                            <TableColumn fx:id="trackResultImgCol" editable="false" prefWidth="75.0" />
                            <TableColumn fx:id="trackResultTitleCol" editable="false" prefWidth="75.0" />
                            <TableColumn fx:id="trackResultArtistCol" editable="false" prefWidth="75.0" />
                            <TableColumn fx:id="trackResultAlbumCol" editable="false" prefWidth="75.0" />
                            <TableColumn fx:id="trackResultLengthCol" editable="false" prefWidth="75.0" />
                            <TableColumn fx:id="trackResultPopCol" editable="false" prefWidth="75.0" />
                        </columns>
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                        </columnResizePolicy>
                    </TableView>
                    <Button fx:id="albumsResultBtn" mnemonicParsing="false" onAction="#albumsResultBtn_OnAction" text="%albums">
                        <styleClass>
                            <String fx:value="deezer-button" />
                            <String fx:value="deezer-subtitle" />
                        </styleClass>
                        <VBox.margin>
                            <Insets top="65.0" />
                        </VBox.margin>
                    </Button>
                    <FlowPane fx:id="albumsResultsFP" hgap="35.0" />
                    <Button fx:id="artistsResultBtn" mnemonicParsing="false" onAction="#artistsResultBtn_OnAction" text="%artists">
                        <styleClass>
                            <String fx:value="deezer-button" />
                            <String fx:value="deezer-subtitle" />
                        </styleClass>
                        <VBox.margin>
                            <Insets top="65.0" />
                        </VBox.margin>
                    </Button>
                    <FlowPane fx:id="artistsResultsFP" hgap="35.0" />
                    <Button fx:id="playlistsResultsBtn" mnemonicParsing="false" onAction="#playlistsResultsBtn_OnAction" text="%playlists">
                        <styleClass>
                            <String fx:value="deezer-button" />
                            <String fx:value="deezer-subtitle" />
                        </styleClass>
                        <VBox.margin>
                            <Insets top="65.0" />
                        </VBox.margin>
                    </Button>
                    <FlowPane fx:id="playlistsResultsFP" hgap="35.0" />
                    <Button fx:id="profilesResultsBtn" mnemonicParsing="false" onAction="#profilesResultsBtn_OnAction" text="%profiles">
                        <styleClass>
                            <String fx:value="deezer-button" />
                            <String fx:value="deezer-subtitle" />
                        </styleClass>
                        <VBox.margin>
                            <Insets top="65.0" />
                        </VBox.margin>
                    </Button>
                    <FlowPane fx:id="profilesResultsFP" hgap="35.0" />
                    <padding>
                        <Insets bottom="80.0" />
                    </padding>
                </VBox>
            </ScrollPane>
        </Tab>
        <Tab fx:id="trackResultsTab" text="%tracks">
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER">
                <VBox style="-fx-background-color: white;">
                    <HBox alignment="CENTER_LEFT">
                        <VBox.margin>
                            <Insets top="30.0" />
                        </VBox.margin>
                        <Label styleClass="card-header" text="%found" />
                        <Label fx:id="foundTracksLbl" styleClass="card-header" text="0" />
                    </HBox>
                    <TableView fx:id="foundTracksTV" VBox.vgrow="NEVER">
                        <columns>
                            <TableColumn fx:id="foundTrAlbImgCol" maxWidth="43.0" minWidth="28.0" prefWidth="-1.0" resizable="false" />
                            <TableColumn fx:id="foundTrNameCol" maxWidth="-Infinity" minWidth="100.0" prefWidth="-1.0" text="%track" />
                            <TableColumn fx:id="foundTrArtistCol" maxWidth="173.0" minWidth="100.0" prefWidth="-1.0" text="%artist" />
                            <TableColumn fx:id="foundTrAlbumCol" maxWidth="-Infinity" minWidth="100.0" prefWidth="-1.0" text="%album" />
                            <TableColumn fx:id="foundTrLengthCol" maxWidth="-Infinity" minWidth="100.0" prefWidth="-1.0" text="%length" />
                            <TableColumn fx:id="foundTrPopCol" maxWidth="-Infinity" minWidth="100.0" prefWidth="-1.0" text="%popularity" />
                        </columns>
                        <columnResizePolicy>
                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                        </columnResizePolicy>
                    </TableView>
                    <padding>
                        <Insets bottom="80.0" left="80.0" right="80.0" />
                    </padding>
                </VBox>
            </ScrollPane>
        </Tab>
        <Tab fx:id="albumResultsTab" text="%albums">
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER" styleClass="scroll-pane">
                <VBox>
                    <HBox>
                        <VBox.margin>
                            <Insets top="30.0" />
                        </VBox.margin>
                        <Label styleClass="card-header" text="%found">
                            <font>
                                <Font size="23.0" />
                            </font>
                        </Label>
                        <Label fx:id="foundAlbumsLbl" styleClass="card-header" text="0" />
                    </HBox>
                    <FlowPane fx:id="foundAlbumsFP" columnHalignment="CENTER" hgap="35.0" maxHeight="1.7976931348623157E308" prefWrapLength="200.0" styleClass="flow-pane" vgap="30.0" />
                    <padding>
                        <Insets bottom="80.0" left="80.0" right="80.0" />
                    </padding>
                </VBox>
                <padding>
                    <Insets bottom="20.0" />
                </padding>
            </ScrollPane>
        </Tab>
        <Tab fx:id="artistResultsTab" text="%artists">
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER" styleClass="scroll-pane">
                <VBox maxHeight="1.7976931348623157E308">
                    <HBox>
                        <VBox.margin>
                            <Insets top="30.0" />
                        </VBox.margin>
                        <Label styleClass="card-header" text="%found">
                            <font>
                                <Font size="23.0" />
                            </font>
                        </Label>
                        <Label fx:id="foundArtistsLbl" styleClass="card-header" text="0" />
                    </HBox>
                    <FlowPane fx:id="foundArtistsFP" columnHalignment="CENTER" hgap="35.0" maxHeight="1.7976931348623157E308" prefWrapLength="200.0" styleClass="flow-pane" vgap="30.0" />
                    <padding>
                        <Insets bottom="80.0" left="80.0" right="80.0" />
                    </padding>
                </VBox>
            </ScrollPane>
        </Tab>
        <Tab fx:id="playlistResultsTab" text="%playlists">
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER" styleClass="scroll-pane">
                <VBox maxHeight="1.7976931348623157E308">
                    <HBox>
                        <VBox.margin>
                            <Insets top="30.0" />
                        </VBox.margin>
                        <Label styleClass="card-header" text="%found">
                            <font>
                                <Font size="23.0" />
                            </font>
                        </Label>
                        <Label fx:id="foundPlaylistsLabel" styleClass="card-header" text="0" />
                    </HBox>
                    <FlowPane fx:id="foundPlaylistsFP" columnHalignment="CENTER" hgap="35.0" maxHeight="1.7976931348623157E308" prefWrapLength="200.0" styleClass="flow-pane" vgap="30.0" />
                    <padding>
                        <Insets bottom="80.0" left="80.0" right="80.0" />
                    </padding>
                </VBox>
            </ScrollPane>
        </Tab>
        <Tab fx:id="mixResultsTab" closable="false" text="%mixes">
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER" styleClass="scroll-pane">
                <VBox maxHeight="1.7976931348623157E308">
                    <HBox>
                        <VBox.margin>
                            <Insets top="30.0" />
                        </VBox.margin>
                        <Label styleClass="card-header" text="%found">
                            <font>
                                <Font size="23.0" />
                            </font>
                        </Label>
                        <Label fx:id="foundMixesLbl" styleClass="card-header" text="0" />
                    </HBox>
                    <FlowPane fx:id="foundMixesFP" columnHalignment="CENTER" hgap="35.0" maxHeight="1.7976931348623157E308" prefWrapLength="200.0" styleClass="flow-pane" vgap="30.0" />
                    <padding>
                        <Insets bottom="80.0" left="80.0" right="80.0" />
                    </padding>
                </VBox>
            </ScrollPane>
        </Tab>
        <Tab fx:id="profileResultsTab" closable="false" text="%profiles">
            <ScrollPane fitToWidth="true" hbarPolicy="NEVER" styleClass="scroll-pane">
                <VBox maxHeight="1.7976931348623157E308">
                    <HBox>
                        <VBox.margin>
                            <Insets top="30.0" />
                        </VBox.margin>
                        <Label styleClass="card-header" text="%found">
                            <font>
                                <Font size="23.0" />
                            </font>
                        </Label>
                        <Label fx:id="foundProfilesLbl" styleClass="card-header" text="0" />
                    </HBox>
                    <FlowPane fx:id="foundProfilesFP" columnHalignment="CENTER" hgap="35.0" maxHeight="1.7976931348623157E308" prefWrapLength="200.0" styleClass="flow-pane" vgap="30.0" />
                    <padding>
                        <Insets bottom="80.0" left="80.0" right="80.0" />
                    </padding>
                </VBox>
            </ScrollPane>
        </Tab>
    </TabPane>
</fx:root>
